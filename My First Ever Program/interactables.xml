<interactables>
  <interactable>
    <Name>Cave</Name>
    <DiscoveredString>You spot a dark, dank cave.</DiscoveredString>
    <InteractionTable>
      <Interaction>
        <InteractionKey>look</InteractionKey>
        <InteractionValue>You decide to peek into the cave mouth.</InteractionValue>
        <InteractionAction>Investigate</InteractionAction>
      </Interaction>

      <Interaction>
        <InteractionKey>leave</InteractionKey>
        <InteractionValue>You turn tail and run the other direction.</InteractionValue>
        <InteractionAction>Leave</InteractionAction>
      </Interaction>
    </InteractionTable>
  </interactable>

  <interactable>
    <Name>Shack</Name>
    <DiscoveredString>A shack it seems</DiscoveredString>
    <InteractionTable>
      <Interaction>
        <InteractionKey>look</InteractionKey>
        <InteractionValue>You decide to peek into the shack</InteractionValue>
        <InteractionAction>Investigate</InteractionAction>
      </Interaction>

      <Interaction>
        <InteractionKey>leave</InteractionKey>
        <InteractionValue>You leave the sinister shack alone</InteractionValue>
        <InteractionAction>Leave</InteractionAction>
      </Interaction>
    </InteractionTable>
  </interactable>

</interactables>
